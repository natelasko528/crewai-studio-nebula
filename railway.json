{
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "startCommand": "sh setup.sh && streamlit run app.py --server.port=$PORT --server.address=0.0.0.0",
    "healthcheckPath": "/",
    "healthcheckTimeout": 300,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}
